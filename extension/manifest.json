{
  "manifest_version": 3,
  "name": "Tracker Framadate",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://framadate.org/*"],
  "content_scripts": [
    {
      "matches": ["https://framadate.org/*"],
      "js": ["content.js"]
    }
  ]
}
